# Required
TEE_PRIVATE_KEY_B64=   # Base64-encoded X25519 private key (never commit real value)
TEE_PUBLIC_KEY_B64=    # Base64-encoded X25519 public key (ok to share)
EVM_RPC=https://evmrpc-testnet.0g.ai  # working RPC (verify)
WALLET_PRIVATE_KEY=    # EVM hex private key for on-chain ops (never commit)
CONTRACT_ADDRESS=      # Your FLAIComputeJobs or hash-registry contract

# Optional storage settings
ZERO_G_STORAGE_UPLOAD_URL=
ZERO_G_STORAGE_API_KEY=

# Behavior toggles
DRY_RUN=1              # set to 0 to actually send transactions
LOCAL_UPLOAD=1         # 1 = store ciphertext under ./uploads as a fallback
# // OG_STORAGE_MODE=ipfs-api          // storage driver: "ipfs-api" (Kubo-compatible /api/v0/add)
# // OG_STORAGE_API_BASE=              // e.g., https://ipfs.0g.ai or http://127.0.0.1:5001
# // OG_STORAGE_API_TOKEN=             // optional bearer token if the API is protected
# // OG_GATEWAY_BASE=                  // e.g., https://ipfs.0g.ai/ipfs; used only for pretty printing links
# // FL_ENC_MODE=dev                        // dev | off
# // FL_TEE_PUBKEY_B64=                     // Base64 X25519 public key (receiver)
# // FL_TEE_PRIVKEY_B64=                    // Base64 X25519 private key (only for TEE-sim decryption)
# // FL_CLIENT_PRIVKEY_B64=                 // Optional sender private key (Base64); if empty, use ephemeral
# // FL_ENC_DELETE_PLAINTEXT=0              // 1 to delete plaintext after encryption (use with care)

# // TEE_ATTEST_MEAS_ALLOWLIST=             // Comma-separated hex measurements allowed (placeholder)
# // TEE_ATTEST_ENCLAVE_ID=my-tee-sim       // Free-form identifier for simulated enclave
